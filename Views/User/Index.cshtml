@{
    ViewData["Title"] = "User Dashboard";
}

<div class="container mt-4">
    <div class="card">
        <div class="card-body">
            <h3 class="card-title">Welcome, @ViewBag.UserName!</h3>
            <p class="card-text">You are in the <strong>@ViewBag.Dashboard</strong>.</p>
            <a href="/" class="btn btn-primary">Go Home</a>
        </div>
    </div>
</div>
